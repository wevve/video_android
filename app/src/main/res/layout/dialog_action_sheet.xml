<?xml version="1.0" encoding="utf-8"?>
<com.sdsmdg.harjot.materialshadows.MaterialShadowViewWrapper
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
            android:id="@+id/ll_content"
            android:orientation="vertical"
            android:dividerPadding="28dp"
            android:divider="@drawable/shape_divider_70"
            android:showDividers="middle|end"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
                android:gravity="center_vertical"
                android:id="@+id/tv_title"
                android:includeFontPadding="false"
                android:paddingLeft="39dp"
                android:text="支付方式"
                android:textColor="#ff333333"
                android:textSize="17dp"
                android:layout_width="wrap_content" android:layout_height="68dp"/>


    </LinearLayout>
</com.sdsmdg.harjot.materialshadows.MaterialShadowViewWrapper>
